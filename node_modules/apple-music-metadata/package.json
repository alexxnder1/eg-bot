{
  "name": "apple-music-metadata",
  "version": "1.0.10",
  "description": "gets apple music metadata for songs and playlists links",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "keywords": [
    "music",
    "apple music",
    "apple music metadata"
  ],
  "author": "night_hunter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Night-Hunter-NF/apple-music-metadata"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "axios-retry": "^3.2.0",
    "cheerio": "^1.0.0-rc.10",
    "htmlparser2": "^7.1.2"
  },
  "devDependencies": {
    "@types/domhandler": "^2.4.2",
    "@types/htmlparser2": "^3.10.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
